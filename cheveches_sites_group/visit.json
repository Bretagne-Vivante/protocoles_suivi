{
  "display_properties": [
    "visit_date_min",
    "num_passage",
    "id_base_site",
    "observers",
    "id_nomenclature_vent",
    "id_nomenclature_meteo",
    "time_observation",
    "id_nomenclature_statut_obs",
    "comments"
  ],
  "display_list": [
    "visit_date_min",
    "num_passage",
    "id_base_site",
    "observers",
    "time_observation",
    "id_nomenclature_statut_obs"
  ],
  "display_form": [
    "id_nomenclature_statut_obs",
    "id_base_site",
    "time_observation"
  ],
  "keep": [
    "num_passage",
    "visit_date_min",
    "id_nomenclature_meteo",
    "id_nomenclature_vent",
    "observers",
    "id_dataset",
    "comment"
  ],
  "specific": {
    "id_base_site": {
      "type_widget": "datalist",
      "attribut_label": "Site",
      "type_util": "site",
      "keyValue": "id_base_site",
      "keyLabel": "base_site_name",
      "api": "__MONITORINGS_PATH/list/__MODULE.MODULE_CODE/site",
      "params": [
        "({meta}) => ({",
          "id_module: __MODULE.ID_MODULE,",
          "id_sites_group: meta.parents.site && meta.parents.site.properties.id_sites_group",
      "})"
      ],
      "application": "GeoNature",
      "required": true,
      "hidden": "({meta, value}) => !meta.bChainInput && value.id_base_site"
    },
    "num_passage": {
      "type_widget": "number",
      "attribut_label": "N° de passage",
      "description": "Préciser si c'est le premièr (1) ou deuxième passage (2)",
      "min": 1,
      "max": 2
    },
    "id_nomenclature_meteo": {
      "type_widget": "select",
      "attribut_label": "Meteo : Ciel",
      "values": ["Indéterminé", "Nuageux", "Moyennement nuageux", "Clair"]
    },
    "id_nomenclature_vent": {
      "type_widget": "select",
      "attribut_label": "Meteo : Vent",
      "values": ["Indéterminé", "Fort", "Modéré", "Faible", "Nul"]
    },
    "visit_date_min": {
      "attribut_label": "Date"
    },
    "cd_nom": {
      "type_widget": "text",
      "attribut_label": "cd nom",
      "type_util": "taxonomy",
      "value": 3511,
      "hidden": true
    },
    "id_nomenclature_statut_obs": {
      "type_widget": "nomenclature",
      "attribut_label": "Observation",
      "code_nomenclature_type": "STATUT_OBS",
      "type_util": "nomenclature",
      "required": true
    },
    "time_observation": {
      "attribut_label": "Heure",
      "type_widget": "time"
    }
  }
}
